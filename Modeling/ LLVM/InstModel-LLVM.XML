<?xml version="1.0" encoding="UTF-8"?>
<!-- XML model for instructions-->


<Instructions>
	<!-- XML model for instruction: Integer arithmetic register-register-->
	<object>
		<type> type </type>
		<name> name </name>	
		<F1> Function 1 </F1>
		<F2> Function 2 </F2>
		<OpCode> OpCode </Opcode>
		<Requires> Requires </Requires> <!-- IsRV32 - Null -->
	</object>

	<!-- XML model for instruction: Integer arithmetic register-immediate-->
	<object>
		<type> type </type>
		<name> name </name>	
		<F1> Function 1 </F1>
		<OpCode> OpCode </Opcode>
		<Imm> immediate </Imm>
		<Requires> Requires </Requires>
	</object>

	<!-- XML model for instruction: NOP, MV, NOT-->
	<object>
		<type> type </type>
		<name> name </name>	
		<Requires> Requires </Requires>
